}

@article{gruzberg2006stochastic,
  title={Stochastic geometry of critical curves, Schramm--Loewner evolutions and conformal field theory},
  author={Gruzberg, Ilya A},
  journal={Journal of Physics A: Mathematical and General},
  volume={39},
  number={41},
  pages={12601},
  year={2006},
  publisher={IOP Publishing},
