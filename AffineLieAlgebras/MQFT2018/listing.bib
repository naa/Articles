@article{hongler2009dipolar,
  title={Dipolar SLE in Ising model with plus-minus-free boundary conditions},
  author={Hongler, Cl{\'e}ment and Kyt{\"o}l{\"a}, Kalle},
  journal={preprint},
  year={2009},
  publisher={Citeseer},
  file={file:///home/anton/Dropbox/LatticeModels/dipolar-ising.pdf:pdf},
  md5sum={87f3e6c433be050b3732bdf28a02f285},
  url={http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.177.613&rep=rep1&type=pdf},
  citations={6},
  citedbyid={14185097573365329102},
  abstract={We study the scaling limit of the Ising model on the square lattice at
criticality in the presence of free boundary conditions. The interface
arising in a setup with consecutive –/+/free boundary conditions has
been conjectured in [BBH05] to converge to an SLEκ ρ process with κ =
3 and ρ = –3/2, also called the dipolar SLE3 . The conjecture
generalizes a result of Smirnov [Smi09b]. We give the first part of
the proof of this conjecture; certain technical details will be
treated in a separate paper. Our proof relies on the introduction of a
new martingale observable and on the computation of its scaling limit.
The martingale property of the observable is established using
KramersWannier duality. The calculation of the scaling limit uses
exact formulae for the one-point and two-point boundary spin
correlation functions in a +/free boundary condition setup. We obtain
these exact formulae by combining two existing fermionic observable
convergence results [Smi07] and [CS09], the scaling limit of FK-Ising
interfaces [Smi09a] and conformal field theory inspired SLE
computations.  Contents},
  pdfmeat={timestamp: 2014-01-11 00:11:10; queries: 1; inode: 1073505}
}

