@article{beaton2012critical,
  title={The critical surface fugacity of self-avoiding walks on a rotated honeycomb lattice},
  author={Beaton, Nicholas R},
  journal={arXiv preprint arXiv:1210.0274},
  year={2012},
  file={file:///home/anton/Dropbox/LatticeModels/1210.0274.pdf:pdf},
  md5sum={ff4f68b9ac1dc7a23f598b67353e7c41},
  url={http://arxiv.org/abs/1210.0274},
  citations={3},
  citedbyid={17036723784238427103},
  abstract={In a recent paper by Beaton et al, it was proved that a model of self-
avoiding walks on the honeycomb lattice, interacting with an
impenetrable surface, undergoes an adsorption phase transition √ when
the surface fugacity is 1 + 2. Their proof used a generalisation of an
identity obtained by Duminil-Copin and Smirnov, and confirmed a
conjecture of Batchelor and Yung. We consider a similar model of self-
avoiding walk adsorption on the honeycomb lattice, but with the
lattice rotated by π/4. For this model there also exists a conjecture
for the critical surface fugacity, made in 1998 by Batchelor, Bennett-
Wood and Owczarek. Using similar methods to Beaton et al, we prove
that this is indeed the critical fugacity.  1},
  pdfmeat={timestamp: 2014-01-11 00:07:50; queries: 2; inode: 1662984}
}

